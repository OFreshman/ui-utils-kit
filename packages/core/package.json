{
  "name": "ui-utils-kit",
  "type": "module",
  "version": "1.1.6",
  "description": "ui-utils-kit 是一个高效的偏业务前端工具函数库",
  "author": "Ethan Yin",
  "license": "MIT",
  "homepage": "https://github.com/OFreshman/ui-utils-kit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OFreshman/ui-utils-kit.git"
  },
  "bugs": "https://github.com/OFreshman/ui-utils-kit/issues",
  "keywords": [
    "js",
    "web",
    "tool",
    "utils"
  ],
  "sideEffects": false,
  "exports": {

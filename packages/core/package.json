  },
  "peerDependencies": {
    "html2canvas-pro": "^1"
  },
  "peerDependenciesMeta": {
    "html2canvas-pro": {
      "optional": true
    }
  },
  "devDependencies": {
    "html2canvas-pro": "^1.5.8"
  }
